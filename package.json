{
  "name": "nivo",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "build": "swc src -d dist --strip-leading-paths",
    "format": "prettier -w \"{src,test}/**/*.ts\"",
    "start": "tsx src/main",
    "start:dev": "tsx watch src/main",
    "start:prod": "node dist/main",
    "test": "vitest run",
    "test:cov": "vitest run --coverage"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@swc/cli": "^0.5.1",
    "@swc/core": "^1.9.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.9.1",
    "@vitest/coverage-v8": "^2.1.5",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5"
  }
}
